{
  "name": "velo-pt-two",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cd app && next dev",
    "build": "cd app && next build",
    "lint": "next lint",
    "start:frontend": "cd app && next start --port 3000",
    "start:backend": "cd backend && node server.js",
    "start": "concurrently \"npm run start:frontend\" \"npm run start:backend\"",
    "dev:frontend": "cd app && next dev",
    "dev:backend": "cd backend && nodemon server.js",
    "dev:all": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\""
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "clsx": "^2.1.1",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-validator": "^7.2.0",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "mongodb": "^6.9.0",
    "mongoose": "^8.6.2",
    "next": "14.2.8",
    "react": "^18",
    "react-dom": "^18",
    "react-modal": "^3.16.1",
    "react-router-dom": "^6.26.1",
    "react-slick": "^0.30.2",
    "slick-carousel": "^1.8.1"
  },
  "devDependencies": {
    "nodemon": "^3.1.4",
    "postcss": "^8",
    "tailwindcss": "^3.4.1"
  }
}
